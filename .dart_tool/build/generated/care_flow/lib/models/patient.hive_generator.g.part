class PatientAdapter extends TypeAdapter<Patient> {
  @override
  final int typeId = 0;

  @override
  Patient read(BinaryReader reader) {
    final numOfFields = reader.readByte();
    final fields = <int, dynamic>{
      for (int i = 0; i < numOfFields; i++) reader.readByte(): reader.read(),
    };
    return Patient(
      id: fields[0] as String,
      name: fields[1] as String,
      email: fields[2] as String,
      age: fields[3] as String,
      gender: fields[4] as String,
      contact: fields[5] as String,
      address: fields[6] as String,
      condition: fields[7] as String,
      medications: (fields[8] as List).cast<String>(),
      treatmentHistory: (fields[9] as List).cast<String>(),
      notes: (fields[10] as List).cast<String>(),
      imageUrls: (fields[11] as List).cast<String>(),
      lastVisit: fields[12] as String,
      emergencyContactName: fields[13] as String?,
      emergencyContactNumber: fields[14] as String?,
      createdAt: fields[15] as DateTime,
      nurseId: fields[16] as String?,
      status: fields[17] as String,
      latitude: fields[18] as double?,
      longitude: fields[19] as double?,
      locationName: fields[20] as String?,
      dob: fields[21] as DateTime?,
      calculatedAge: fields[22] as int?,
      distanceFromNurse: fields[23] as double?,
    );
  }

  @override
  void write(BinaryWriter writer, Patient obj) {
    writer
      ..writeByte(24)
      ..writeByte(0)
      ..write(obj.id)
      ..writeByte(1)
      ..write(obj.name)
      ..writeByte(2)
      ..write(obj.email)
      ..writeByte(3)
      ..write(obj.age)
      ..writeByte(4)
      ..write(obj.gender)
      ..writeByte(5)
      ..write(obj.contact)
      ..writeByte(6)
      ..write(obj.address)
      ..writeByte(7)
      ..write(obj.condition)
      ..writeByte(8)
      ..write(obj.medications)
      ..writeByte(9)
      ..write(obj.treatmentHistory)
      ..writeByte(10)
      ..write(obj.notes)
      ..writeByte(11)
      ..write(obj.imageUrls)
      ..writeByte(12)
      ..write(obj.lastVisit)
      ..writeByte(13)
      ..write(obj.emergencyContactName)
      ..writeByte(14)
      ..write(obj.emergencyContactNumber)
      ..writeByte(15)
      ..write(obj.createdAt)
      ..writeByte(16)
      ..write(obj.nurseId)
      ..writeByte(17)
      ..write(obj.status)
      ..writeByte(18)
      ..write(obj.latitude)
      ..writeByte(19)
      ..write(obj.longitude)
      ..writeByte(20)
      ..write(obj.locationName)
      ..writeByte(21)
      ..write(obj.dob)
      ..writeByte(22)
      ..write(obj.calculatedAge)
      ..writeByte(23)
      ..write(obj.distanceFromNurse);
  }

  @override
  int get hashCode => typeId.hashCode;

  @override
  bool operator ==(Object other) =>
      identical(this, other) ||
      other is PatientAdapter &&
          runtimeType == other.runtimeType &&
          typeId == other.typeId;
}
